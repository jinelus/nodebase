{
  "name": "front-end",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm env:load vite build && node -e \"const fs = require('fs'); if (fs.existsSync('.output/server')) { fs.copyFileSync('instrument.server.mjs', '.output/server/instrument.server.mjs') }\"",
    "db:generate": "pnpm env:load drizzle-kit generate",
    "db:migrate": "pnpm env:load drizzle-kit migrate",
    "db:push": "pnpm env:load drizzle-kit push",
    "db:seed": "pnpm env:load tsx ./src/db/seed.ts",
    "db:studio": "pnpm env:load drizzle-kit studio",
    "dev": "pnpm env:load cross-env NODE_OPTIONS=--import=./instrument.server.mjs vite dev --port 3000",
    "format": "biome format --write",
    "lint": "biome check --write",
    "ngrok:dev": "ngrok http --url=https://quaternate-theoretically-elba.ngrok-free.dev 3000",
    "serve": "vite preview",
    "test": "vitest run",
    "trigger:cli": "pnpm env:load pnpm dlx trigger.dev dev",
    "env:load": "dotenv -e ../../.env --",
    "preview": "pnpm env:load node .output/server/index.mjs"
  },
